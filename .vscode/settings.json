{
  "editor.tabCompletion": "on",
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.tmp": true,
    "**/dist": true,
    "**/build": true
  },
  "css.lint.duplicateProperties": "warning",
  "css.lint.universalSelector": "warning",
  "scss.lint.duplicateProperties": "warning",
  "html.format.endWithNewline": true,
  "editor.tabSize": 2,
  "html.format.unformatted": "a, abbr, acronym, b, bdo, big, br, button, cite, code, dfn, em, i, img, input, kbd, label, map, object, q, samp, select, small, span, strong, sub, sup, textarea, tt, var",
  "files.associations": {
    "*.vue": "javascript"
  },
  "eslint.validate": ["javascript", "javascriptreact", "vue"],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "workbench.colorTheme": "One Monokai",
  "workbench.iconTheme": "file-icons",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.panel.defaultLocation": "right",
  "workbench.colorCustomizations": {
    "terminal.background": "#212121",
    "terminal.foreground": "#EEFFFF",
    "terminalCursor.background": "#EEFFFF",
    "terminalCursor.foreground": "#EEFFFF",
    "terminal.ansiBlack": "#212121",
    "terminal.ansiBlue": "#82AAFF",
    "terminal.ansiBrightBlack": "#4A4A4A",
    "terminal.ansiBrightBlue": "#82AAFF",
    "terminal.ansiBrightCyan": "#89DDFF",
    "terminal.ansiBrightGreen": "#C3E88D",
    "terminal.ansiBrightMagenta": "#C792EA",
    "terminal.ansiBrightRed": "#F07178",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#FFCB6B",
    "terminal.ansiCyan": "#89DDFF",
    "terminal.ansiGreen": "#C3E88D",
    "terminal.ansiMagenta": "#C792EA",
    "terminal.ansiRed": "#F07178",
    "terminal.ansiWhite": "#EEFFFF",
    "terminal.ansiYellow": "#FFCB6B"
  },
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "editor.fontSize": 13,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "keyboard.layout": "com.apple.keylayout.Italian-Pro",
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "workbench.startupEditor": "none",
  "omnisharp.useGlobalMono": "always",
  "omnisharp.useEditorFormattingSettings": false,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp",
    "editor.tabSize": 4
  }
}
